  "dependencies": {
    "@google/genai": "^0.10.0",
    "@residency/eslint-config": "workspace:*",
    "@residency/tsconfig": "workspace:*",
    "convex": "catalog:",
    "elevenlabs": "^1.57.0",
    "nanoid": "^5.1.5"
  },
  "devDependencies": {
    "@residency/tsconfig": "workspace:*",
    "@types/node": "^22.14.1",
    "typescript": "catalog:"
  }
